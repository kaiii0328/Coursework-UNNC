/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
	Or(a=a[0],b=b[0],out=out[0]);
	Or(a=a[1],b=b[1],out=out[1]);
	Or(a=a[2],b=b[2],out=out[2]);
	Or(a=a[3],b=b[3],out=out[3]);
}